  <rdf:Description rdf:about="http://example.org/data/item/152">
    <rdf:type rdf:resource="http://example.org/data/Item"/>
    <ns1:indice rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">125</ns1:indice>
    <ns1:prompt rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Taxonomía y provincias donde aparecen</ns1:prompt>
    <ns1:sql rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT
    t.name AS NombreCientifico,
    t.taxonrank AS RangoTaxonomico,
    t.kingdom AS Reino,
    t.phylum AS Filo,
    t.class AS Clase,
    t.taxonorder AS Orden,
    t.family AS Familia,
    t.genus AS Genero,
    c.Provincia AS Provincia
FROM
    Taxonomia AS t
INNER JOIN
    CuadriculasEspecies AS ce ON t.taxonid = ce.idtaxon
INNER JOIN
    Cuadriculas AS c ON ce.cuadricula = c.CUADRICULA
WHERE
    t.taxonid = &lt;idtaxon&gt;
ORDER BY
    c.Provincia;</ns1:sql>
    <ns1:split rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Train</ns1:split>
  </rdf:Description>
