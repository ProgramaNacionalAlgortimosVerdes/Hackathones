    <ns1:prompt rdf:datatype="http://www.w3.org/2001/XMLSchema#string">quiero saber a q comunidades tengo q ir para no encontrarme con estas especies</ns1:prompt>
    <ns1:sql rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT DISTINCT CCAA
FROM Cuadriculas
WHERE CCAA NOT IN (
    SELECT DISTINCT c.CCAA
    FROM CuadriculasEspecies ce
    JOIN Cuadriculas c ON ce.cuadricula = c.CUADRICULA
    WHERE ce.idtaxon = &lt;idtaxon&gt;
);</ns1:sql>
