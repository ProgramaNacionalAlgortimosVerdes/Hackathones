    <ns1:sql rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT
    r.ID_Ruta,
    r.Nombre_Ruta,
    r.Nombre_Etapa
FROM
    CuadriculasEspecies ce
INNER JOIN
    CuadriculasRutas cr ON ce.CUADRICULA = cr.CUADRICULA
INNER JOIN
    Rutas r ON cr.ID_Ruta = r.ID_Ruta
WHERE
    ce.idtaxon = &lt;idtaxon&gt;</ns1:sql>
