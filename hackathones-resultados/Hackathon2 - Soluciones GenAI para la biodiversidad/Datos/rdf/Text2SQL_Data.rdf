    <ns1:sql rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT
    ne.nombre_comun AS NombreComun,
    ne.idioma AS IdiomaNombreComun,
    t.name AS NombreCientificoSinonimo
FROM
    NombresEspecies AS ne
LEFT JOIN
    Taxonomia AS t ON ne.idtaxon = t.taxonid
WHERE
    ne.idtaxon = &lt;idtaxon&gt; AND ne.espreferente = 0
UNION ALL
SELECT
    NULL AS NombreComun,
    NULL AS IdiomaNombreComun,
    t.name AS NombreCientificoSinonimo
FROM
    Taxonomia AS t
WHERE
    t.taxonid = &lt;idtaxon&gt; AND t.nametype = 'Sinónimo';</ns1:sql>
