  <rdf:Description rdf:about="http://example.org/data/item/26">
    <rdf:type rdf:resource="http://example.org/data/Item"/>
    <ns1:indice rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">29</ns1:indice>
    <ns1:prompt rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Antes de cada descripción, pon la taxonomía de la especie</ns1:prompt>
    <ns1:sql rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT
    t.taxonid,
    t.name AS nombre_cientifico,
    t.taxonrank,
    t.origen,
    t.taxonomicgroup,
    t.kingdom,
    t.phylum,
    t.class,
    t.taxonorder,
    t.family,
    t.genus,
    t.subgenus,
    t.nametype
FROM
    dbo.Taxonomia AS t
WHERE
    t.taxonid = &lt;idtaxon&gt;;</ns1:sql>
    <ns1:split rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Train</ns1:split>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/data/item/132">
    <rdf:type rdf:resource="http://example.org/data/Item"/>
    <ns1:indice rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">119</ns1:indice>
    <ns1:prompt rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Incluye, por favor, el total de rutas donde ha sido vista cada especie.</ns1:prompt>
    <ns1:sql rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT
    COUNT(DISTINCT r.ID_Ruta) AS TotalRutas
FROM
    Taxonomia AS t
INNER JOIN
    CuadriculasEspecies AS ce ON t.taxonid = ce.idtaxon
INNER JOIN
    CuadriculasRutas AS cr ON ce.cuadricula = cr.CUADRICULA
INNER JOIN
    Rutas AS r ON cr.ID_Ruta = r.ID_Ruta
WHERE
    t.taxonid = &lt;idtaxon&gt;</ns1:sql>
    <ns1:split rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Test</ns1:split>
  </rdf:Description>
