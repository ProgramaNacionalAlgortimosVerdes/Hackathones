    <ns1:prompt rdf:datatype="http://www.w3.org/2001/XMLSchema#string">en q rutas &lt; 15 km se han registrado estas especies?</ns1:prompt>
    <ns1:sql rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT
    r.Nombre_Ruta,
    r.Nombre_Etapa,
    r.Longitud
FROM
    CuadriculasEspecies ce
INNER JOIN
    CuadriculasRutas cr ON ce.CUADRICULA = cr.CUADRICULA
INNER JOIN
    Rutas r ON cr.ID_Ruta = r.ID_Ruta
WHERE
    ce.idtaxon = &lt;idtaxon&gt;
    AND r.Longitud &lt; 15</ns1:sql>
    <ns1:split rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Train</ns1:split>
