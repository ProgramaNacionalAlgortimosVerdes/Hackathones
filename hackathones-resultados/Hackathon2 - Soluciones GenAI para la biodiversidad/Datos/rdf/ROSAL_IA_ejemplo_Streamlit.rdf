    <ns1:scientific_name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gymnogongrus crenulatus</ns1:scientific_name>
    <ns1:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">On the life-history of Gymnogongrus Griffithsiae Mart. and Ahnfeltia plicata Fries</ns1:title>
